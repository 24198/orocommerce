{% extends 'OroUIBundle:actions:index.html.twig' %}
{% import 'OroUIBundle::macros.html.twig' as UI %}

{% set pageTitle = 'oro.coupon.entity_plural_label'|trans %}

{% block navButtons %}

    {% include 'OroImportExportBundle:ImportExport:buttons.html.twig' with {
        entity_class: entity_class,
        exportProcessor: 'oro_coupon_coupon_export',
        importProcessor: 'oro_coupon_coupon_import',
        exportTemplateProcessor: 'oro_coupon_coupon_export_template',
        dataGridName: gridName
    } %}

    {% if resource_granted('oro_coupon_create') %}
        <div class="btn-group pull-right">
            {{ UI.addButton({
                'path': path('oro_coupon_create'),
                'entity_label': 'oro.coupon.entity_label'|trans
            }) }}
        </div>
    {% endif %}
{% endblock %}


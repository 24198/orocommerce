{% block _oro_rfp_request_form_title_widget %}
	{% set attr = layout_attr_defaults(attr, {
		'~class': " title title--uppercase"
	}) %}

	<h1{{ block('block_attributes') }}>
		{{ block_widget(block) }}
	</h1>
{% endblock %}
 
{% block _oro_rfp_request_form_container_widget %}
    {% set attr = layout_attr_defaults(attr, {
        '~class': " address-form-outer"
    }) %}
 
    <div{{ block('block_attributes') }}>
        {{ block_widget(block) }}
    </div>
{% endblock %}

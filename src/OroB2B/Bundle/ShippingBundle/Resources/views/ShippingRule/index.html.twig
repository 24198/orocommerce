{% set gridName = 'shipping-rule-grid' %}
{% set pageTitle = 'oro.shipping.shippingrule.entity_plural_label'|trans %}

{% block navButtons %}
    {% if resource_granted('orob2b_shipping_rule_create') %}
        <div class="btn-group pull-right">

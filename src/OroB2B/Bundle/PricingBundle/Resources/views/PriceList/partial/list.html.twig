<div class="clearfix">
    <table class="table">
        <thead>
        <tr>
            <th>#</th>
            <th>{{ 'orob2b.pricing.pricelist.name.label'|trans }}</th>
            <th>{{ 'orob2b.pricing.pricelist.priority.label'|trans }}</th>
        </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td>{{ loop.index }}</td>
                <td><a href="{{ path('orob2b_website_view', {'id': website.id}) }}">{{ entity.priceList.name }}</a></td>
                <td>{{ entity.priority }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>

{% block _request_for_quote_for_unit_widget %}
    {% for unit_precision in unit_precisions %}
        <tr>
            <td colspan="2" class="text-center">
                <strong>{{ unit_precision.unit.code|orob2b_format_product_unit_label|capitalize }}</strong>
            </td>
        </tr>
        <tr>
            <td colspan="2" class="text-center">
                <div class="product_view_request_quote_for_unit">{{ 'orob2b.frontend.sale.btn.add_to_rfp'|trans }}</div>
            </td>
        </tr>
    {% endfor %}
{% endblock %}
